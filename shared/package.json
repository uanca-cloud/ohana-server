{
  "name": "ohana-shared",
  "description": "Shared component for the Ohana project which uses a serverless model with Azure Functions",
  "version": "2.0.0",
  "license": "private",
  "main": "index.js",
  "scripts": {
    "tty-azf": "PID=$(docker ps | grep ohana_azf | awk -F ' ' '{print $1;}') && docker exec -ti $PID /bin/sh",
    "tty-db": "PID=$(docker ps | grep ohana_db | awk -F ' ' '{print $1;}') && docker exec -ti $PID /bin/sh",
    "tty-psql": "PID=$(docker ps | grep ohana_db | awk -F ' ' '{print $1;}') && docker exec -ti $PID psql ohana -U ohana_admin"
  },
  "dependencies": {
    "@azure/app-configuration": "1.4.1",
    "@azure/notification-hubs": "1.2.1",
    "@azure/service-bus": "7.9.1",
    "@azure/storage-blob": "12.12.0",
    "@azure/web-pubsub": "1.1.1",
    "@graphql-tools/mock": "9.0.0",
    "@graphql-tools/schema": "10.0.0",
    "amqplib": "0.10.3",
    "busboy": "1.6.0",
    "date-fns": "2.29.3",
    "generic-pool": "3.9.0",
    "jimp": "0.16.2",
    "jsonwebtoken": "9.0.0",
    "jwk-to-pem": "2.0.5",
    "libphonenumber-js": "1.10.14",
    "lodash.chunk": "4.2.0",
    "lodash.difference": "4.5.0",
    "lodash.groupby": "4.6.0",
    "lodash.partial": "4.2.1",
    "lodash.remove": "4.7.0",
    "lodash.template": "4.5.0",
    "lodash.uniqby": "4.7.0",
    "ohana-azure-functions": "*",
    "pg": "8.8.0",
    "pino": "8.11.0",
    "redact-object": "3.0.1",
    "redis": "4.6.10",
    "ulid": "2.3.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.19.1",
    "del": "6.1.1",
    "dotenv": "16.0.3",
    "eslint": "8.26.0",
    "jest": "29.2.2",
    "jest-junit": "14.0.1",
    "merge-stream": "2.0.0",
    "pino-pretty": "10.0.0",
    "prettier": "2.7.1",
    "semver": "7.5.2",
    "shelljs": "0.8.5",
    "wait-port": "1.0.4",
    "webpack": "5.76.0",
    "webpack-node-externals": "3.0.0"
  }
